a.browse {
	background-image:url("images/scroller_btns.gif");
	background-repeat:no-repeat;
	cursor:pointer;
	display:block;
	float:left;
	font-size:1em;
	height:30px;
	margin:40px 10px;
	padding:0px;
	width:30px;
}
a.up, a.down {
	background-image:url("images/scroller_btns.gif");
	background-repeat:no-repeat;
	float:none;
	height:18px;
	padding:0px;
	width:181px;
}
a.up {
	background-position:-182px 0px;
	margin:3px 0 0px;
}
a.up:hover {
	background-position:0 0px;
}
a.up:active {
	background-position:-360px 0px;
}
a.down {
	background-position:-182px -18px;
	margin:0px;
	padding:0px;
}
a.down:hover {
	background-position:0 -18px;
}
a.down:active {
	background-position:-360px -18px;
}
a.disabled {
	visibility:hidden !important;
}
